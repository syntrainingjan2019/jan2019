<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="helloWorld" class="com.models.HelloWorld" >
	<property name="message1" value="Hello world message1" />
	<property name="message2" value="Hello world message2" />
	</bean>
	<bean id="helloIndia" class="com.models.HelloIndia" parent="helloWorld">
	<property name="message3" value="Hello india message3" />
	
	</bean>
	
	</beans>